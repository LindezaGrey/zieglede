config global
        option anon_swap '0'
        option anon_mount '0'
        option auto_swap '1'
        option auto_mount '1'
        option delay_root '5'
        option check_fs '0'

config mount
        option uuid '11c16739-0987-4294-bb2d-d46fcde322fb'
        option target '/mnt/share'
        option enabled '1'

config mount
        option uuid '9d2cc512-0de9-4ce9-9e5e-560c0d21337d'
        option target '/mnt/backupdrive'
        option enabled '1'

